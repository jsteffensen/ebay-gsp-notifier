{
"manifest_version": 2,
"name": "eBay GSP Notifier",
"description": "This extension turns the Global Shipping Program (GSP) fine print red on eBay items pages, when GSP items are shown",
"version": "1.3",
"content_scripts": [
 {
 "matches": [
	"https://www.ebay.com/*", 
	"https://www.ebay.co.uk/*"
 ],
  "js": ["content.js"],
  "run_at": "document_end"
 }
],
"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" 
},
"permissions": [
              "activeTab"
             ],
"browser_action": {

 }

}